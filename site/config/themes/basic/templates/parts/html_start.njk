<!DOCTYPE html>
<html lang="{{ LANG }}">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>{{ meta.title }} :: {{ config.title }}</title>
  <meta property="og:title" content="{{ meta.title}}" />
  {% if page.meta.image or config.image %}
  {% if page.meta.image %}
  {% set img = page.meta.image%}
  {% else %}
  {% set img = config.image %}
  {% endif %}
  <meta property="og:image" content="{{config.url}}{{img}}" />
  {% endif %}
  <meta property="og:description" content="{%- if meta.excerpt -%}
      {{ meta.excerpt | striptags -}}
      {% else %}{{ config[ " description_" + LANG ]}}{% endif %}" />
  <link href="/_theme/style.css" rel="stylesheet">
  <link href="/_theme/highlight/styles/isbl-editor-dark.css" rel="stylesheet">
  <script src="/_theme/navbar.js">
  </script>
  {{ jsapi }}
  <script src="/search_client.js"></script>
  <script src="/_theme/masonry.js"></script>
  <script src="/_theme/highlight/highlight.min.js"></script>
  <script>hljs.highlightAll();</script>
  <script type="module">
    import mermaid from 'https://cdn.jsdelivr.net/npm/mermaid@11/dist/mermaid.esm.min.mjs';
    mermaid.initialize({startOnLoad: true});
  </script>
</head>

